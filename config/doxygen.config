PROJECT_NAME = "Batteries C++"

OUTPUT_DIRECTORY = ./build/doxygen
XML_OUTPUT = ./xml
GENERATE_XML = YES
GENERATE_HTML = NO
GENERATE_LATEX = NO
HAVE_DOT = NO
INPUT = ./src
INPUT_ENCODING = UTF-8
FILE_PATTERNS = *.c *.cpp *.h *.hpp *.y *.html *.css *.js *.xml *.md
RECURSIVE = YES
EXCLUDE =
EXCLUDE_SYMLINKS = NO
EXCLUDE_PATTERNS = template.hpp *.test.cpp *_test.cpp
FILTER_PATTERNS =
FILTER_SOURCE_FILES = NO
FILTER_SOURCE_PATTERNS =

FULL_PATH_NAMES = YES
INCLUDE_PATH = ./src
STRIP_FROM_INC_PATH = ./src
STRIP_FROM_PATH = ./src
SHORT_NAMES = NO
JAVADOC_AUTOBRIEF = YES
ENABLE_PREPROCESSING = YES
MACRO_EXPANSION = YES
PREDEFINED += BATT_GENERATING_DOCS:=1
EXAMPLE_PATH=
CLANG_DATABASE_PATH = ./build/RelWithDebInfo/compile_commands.json